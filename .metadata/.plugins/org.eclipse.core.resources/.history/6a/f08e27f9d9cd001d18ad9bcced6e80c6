package chap18_enum.eunms;

public enum RGB {
	//상수명 RED("red", 255, 0, 0), 
	//	   GREEN, 
	//     BLUE
	RED("red", 255, 0, 0),
	GREEN("green", 0, 255, 0),
	BLUE("blue", 0, 0, 255);
	
	private String colorStr;
	private int red;
	private int green;
	private int blue;
	
	RGB(String colorStr, int red, int green,
			int blue) {
		this.colorStr = colorStr
	}
	
	
	
	
}
