package chap18_enum.eunms;

public enum Season {
	SPRING("봄") {
		//봄은 3, 4, 5월입니다.
	}, 
	SUMMER("여름") {
		//여름은 6, 7, 8월입니다.
	}, 
	FALL("가을") {
		
	}, 
	WINTER("겨울") {
		
	};
	
	private String koreaSeason;
	
	Season(String koreaSeason) {
		this.koreaSeason = koreaSeason;
	}

	public String getKoreaSeason() {
		return koreaSeason;
	}
	
	public abstract void printSeasonMonth();
}
