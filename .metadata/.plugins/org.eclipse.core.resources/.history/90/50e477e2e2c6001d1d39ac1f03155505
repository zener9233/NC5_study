package chap08_static.clazz;

public class Weight {
	public void twoWeightKgSum() {
		int a = 76;
		int b = 65;
		
		String sum = Add.add(a, b, "kg");
		System.out.println(sum);
		sum = Add.add(a, b, "g");
	}
	
	public void twoWeightGramSum() {
		int a = 76;
		int b = 65;
		System.out.println(sum);
		sum = Add.add(a * 1000, b * 1000, "g");
	}
}
