package chap12_nestedclass;

class NestedClass {
	class Add {
		int num1;
		int num2;
		
		int result;
		
		int add() {
			result = num1 + num2;
			return result;
		}
	}
	
	static class Sub {
		static int num3;
		static int num4;
		
		static int result;
		static int add() {
			result = num3 - num4;
			return result;
		}
	}
}

public class _03_instanceOfNestedClass {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
