package chap13_objectarray.clazz;

public class MathMetics implements ILecture {
	private String subject;
	private int studentCnt;
	private int lectureTime;
	
	public MathMetics(String subject, 
			int studentCnt, int lectureTime) {
		this.subject = subject;
		this.studentCnt = studentCnt;
		this.lectureTime = lectureTime;
	}
	@Override
	public void proceedLecture() {
		// TODO Auto-generated method stub
		System.out.println("수학수업을 50명의 학생이 듣습니다. "
				+ "수업시간은 60분입니다.");
	}

}
