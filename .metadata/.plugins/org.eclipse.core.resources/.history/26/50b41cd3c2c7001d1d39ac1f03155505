package chap12_nestedclass.clazz;

public class Add implements Calc {

	@Override
	public void calculator(int a, int b) {
		// TODO Auto-generated method stub
		new Calc() {

			@Override
			public void calculator(int a, int b) {
				// TODO Auto-generated method stub
				
			}
		};
		
		return a + b;
	}

}
