package chap15_usefulClass;

public class _12_boxing {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num1 = 100;
		
		//                원시타입을 객체로 만들어줌 => 박싱
		Integer objNum1 = (Integer)num1;
		
		//                자동 언박싱
		//                객체를 원시타입으로 변환
		int num2 = num1 + objNum1;
		
		
		
		
		
	}

}
