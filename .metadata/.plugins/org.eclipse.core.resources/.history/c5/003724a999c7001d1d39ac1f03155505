		// TODO Auto-generated method stub
		
	}

	@Override
	public int getTicketPrice(int people) {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public void getRemainSeat() {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void playSportsAndConcert() {
		// TODO Auto-generated method stub
		
	}
}
